@model IEnumerable<SismaV02.Models.Calle>

@{
    ViewBag.Title = "Calles";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomCalle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Barrio.NomBarrio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomCalle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Barrio.NomBarrio)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.CodCalle }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.CodCalle }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.CodCalle })
        </td>
    </tr>
}

</table>
